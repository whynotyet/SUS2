t.test(ori_sus$p6, rev_sus$p6) #not
t.test(ori_sus$p7, rev_sus$p7)
t.test(ori_sus$p8, rev_sus$p8) #not
t.test(ori_sus$p9, rev_sus$p9)
t.test(ori_sus$p10, rev_sus$p10)

t.test(rowMeans(ori_sus), rowMeans(rev_sus))

